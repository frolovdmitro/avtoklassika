<li class="payments-deliveries__item"> {{IF basket}} <input type="radio" name="delivery" id="delivery-{{id}}" data-usd-cost="{{cost_usd_delivery}}" data-cost="{{cost_delivery_unformat}}" class="payments-deliveries__radio" value="{{id}}"> {{END IF}} <label for="delivery-{{id}}" class="payments-deliveries__link _delivery-link {{if(_last, ' payments-deliveries__link_position_last')}}"><img class="payments-deliveries__image" src="/img/deliveries/{{type}}.png" alt="" width="71" height="57"> {{name}} {{IF description}} <span data-hint="{{description}}" class="hint--top{{if(multiline, ' hint--multiline')}} icons__help"></span> {{END IF}} </label> </li>