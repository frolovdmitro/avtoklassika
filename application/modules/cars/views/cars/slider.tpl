{{IF cars}}<div class="nav_filter" id="slider_level">  <ul class="creations_filter owl-carousel owl-theme">    {{BEGIN cars}}      <li class="item">        <div class="image {{if(current, 'current_active')}}" id="firstfilter"">          <div class="img">            <a href="/car/{{synonym}}/" class="{{if(current, 'current')}}" id="firstfilter">              <img class="img img-responsive center-block" src="{{url_staticServer}}{{image}}" alt="{{name}}">            </a>          </div>          <div class="caption">{{name}}</div>        </div>      </li>    {{END cars}}  </ul></div>{{END IF}}