			</files>
		</add>
	</validate>
	<index>
		<title></title>
		<keywords></keywords>
		<description></description>
	</index>
	<templates>
		<title>{{IF title}}{{title}}{{else}}{{type}}: {{name}}{{IF cost}}, цена {{cost}}€{{END IF}} ({{city}})
			{{END IF}}{{IF vars}}
			{{name}} - название объявления
			{{cost}} - цена
			{{type}} - Продам/Куплю
			{{category}} - Автозапчасть/Автомобиль
			{{date}} - Дата
			{{country}} - Страна
			{{city}} - Город
			{{title}} - TITLE
			{{keywords}} - KEYWORDS
			{{description}} - DESCRIPTION
			{{END IF}}
		</title>
	  <keywords>{{name}}</keywords>
	  <description>{{name}}</description>
	  <share><![CDATA[		  {{name}}
preis: {{cost}} {{IF currency_abb == 'P'}}Euro.{{ELSE}}{{currency_abb}}{{END IF}}
------------------------------------
http://avtoclassika.com/car/{{car_synonym}}/{{autopart_id}}/{{id}}/

{{description}}
	  ]]></share>
	</templates>
