	<validate>
		<add>
			<name>
				<empty>Ungültige name</empty>
			</name>
			<city>
				<empty>Ungültige Stadt, Land</empty>
			</city>
			<email>
				<empty>Sie haben nicht die angegebene E-mail</empty>
				<parseEmail>Ungültige E-mail</parseEmail>
			</email>
			<phone>
				<empty>Sie haben nicht die angegebene Telefon</empty>
			</phone>
			<type>
				<empty>Wählen  Sie die Art der Anzeige</empty>
			</type>
			<category>
				<empty>Wählen Sie die Rubrik</empty>
			</category>
			<caption>
				<empty>Sie haben keinen Überschrift eingefügt</empty>
			</caption>
			<text>
				<empty>Sie haben keine Anzeigentext eingefügt</empty>
			</text>
